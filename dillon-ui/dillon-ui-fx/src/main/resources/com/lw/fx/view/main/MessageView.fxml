<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox prefHeight="449.0" prefWidth="334.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lw.fx.view.main.MessageView">
   <children>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab text="我的消息">
               <content>
                  <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" style="-fx-border-width: 0;" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <HBox alignment="CENTER">
         <children>
            <Hyperlink fx:id="markersBut" text="标记已读" />
            <Region HBox.hgrow="ALWAYS" />
            <Hyperlink fx:id="viewAllBut" text="查看全部">
               <graphic>
                  <FontIcon iconLiteral="fth-eye" />
               </graphic>
            </Hyperlink>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
